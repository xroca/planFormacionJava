<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:c="http://www.springframework.org/schema/c"
       xmlns:p="http://www.springframework.org/schema/p"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
">
    <bean id="aficion" class="com.curso.piloto.beans.Aficion" c:nombre="Afición importado"/>
    <bean id="aficion1" class="com.curso.piloto.beans.Aficion" p:nombre="Afición importado"/>
    <bean id="persona" class="com.curso.piloto.beans.Persona" c:nombre="Persona importado" c:aficion-ref="aficion"/>
</beans>
